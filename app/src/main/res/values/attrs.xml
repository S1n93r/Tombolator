<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PaginatedMediaList">
        <attr name="title" format="string"/>
        <attr name="elementsPerPage" format="integer"/>
        <attr name="processButtonText" format="string"/>
